/**app.wxss**/
@import "/assets/img/font/iconfont.wxss";
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 
.page{
  background-color: #f5f5f5;
}
.masonry {
  column-count: 2;
  column-gap: 20rpx;
}

.item {
  break-inside: avoid;
  box-sizing: border-box;
  padding: 20rpx;
  break-inside: avoid;
  
}
.item-left {
  /* padding-right: 10rpx; */
  padding-right: 0rpx;
  padding-top: 0rpx;
}
.item-left:first-child {
  padding-top: 20rpx;
}
.item-right:first-child{
  padding-bottom: 20rpx;
}
.item-right{
  /* padding-left: 10rpx; */
  padding-left: 0rpx;
  padding-bottom: 0rpx;

}