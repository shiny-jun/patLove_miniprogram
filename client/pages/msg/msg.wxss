/* pages/msg/msg.wxss */
.page{
    overflow: hidden;
    background-color: #eeeeee;
    width: 100%;
    height: 100vh;
}
.item-wrapper{
    box-shadow: 0px 0px 4px 0px rgba(65, 47, 141, 0.2);
    margin-top: 30rpx;
}
.item{
    border-bottom: 1rpx solid #eeeeee;
    height: 68rpx;
    line-height: 68rpx;
    padding:0 20rpx;
    color: #999999;
    font-size: 32rpx;
    background: #ffffff;
}
.item:last-child{
    border-bottom:none;
}
.item .icon-right{
    float: right;
    font-size: 24rpx;
}